{
    "version": 2,
    "rewrites": [{
            "source": "/api/:path*",
            "destination": "/api/server.js"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/frontend/dist/assets/$1"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/frontend/dist/index.html"
        }
    ]
}